{
  "name": "personal-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "prepare": "husky",
    "format:check": "prettier --check .",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:js:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "lint:css": "stylelint \"**/*.module.css\"",
    "lint:css:fix": "stylelint \"**/*.module.css\" --fix"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,css,scss,md}": [
      "npm run lint",
      "npm run format:check"
    ]
  },
  "dependencies": {
    "classnames": "2.5.1",
    "next": "16.0.10",
    "next-intl": "4.5.8",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/postcss": "4",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "eslint": "9",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "stylelint-config-css-modules": "4.5.1",
    "stylelint-config-recess-order": "7.4.0",
    "stylelint-config-standard": "39.0.1",
    "tailwindcss": "4",
    "typescript": "5.9.3"
  }
}
